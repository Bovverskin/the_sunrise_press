.cms_index_container
    -if current_user.role == 'editor'
        %h1.m-4.text-red-darkest Non-published articles
        = render partial: 'articles_list', locals: {articles: @articles, published_state: :not_published}
        %h1.m-4.text-red-darkest Published articles
        = render partial: 'articles_list', locals: {articles: @articles, published_state: :published}